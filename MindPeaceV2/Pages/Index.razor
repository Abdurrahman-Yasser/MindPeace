@page "/"

<PageTitle>Home</PageTitle>

<div class="col p-0 m-0">
    <!--Image on top-->
    <div id="carouselExampleCaptions" class="carousel slide">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                    aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                    aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                    aria-label="Slide 3"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fpillarsofwellness.ca%2Fwp-content%2Fuploads%2F2021%2F03%2Fmental-health.jpg&f=1&nofb=1&ipt=0010366224c6faafa372cda5ab60b949c0e41b63d81ee14ab747a43960ccda3f&ipo=images" width="1200" height="500" class="d-block w-100">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Free Depression Test</h5>
                    <p>check if you have depression</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fapi.hub.jhu.edu%2Ffactory%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fsoft_crop_2400%2Fpublic%2Fmental_health_040620.jpg%3Fitok%3DeObhgWGI&f=1&nofb=1&ipt=2ce4a2ab63df6595f714be17ed87e8f8053cd4c7eff3fca27a0257ec2eed3c65&ipo=images" width="1200" height="500" class="d-block w-100">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Reach Out to a Specialist</h5>
                    <p>contact a mental health expert to discuss your depression</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fd2jx2rerrg6sh3.cloudfront.net%2Fimages%2FArticle_Images%2FImageForArticle_22028_16430272124432966.jpg&f=1&nofb=1&ipt=06a6c2af91ed21d5d58823fe3736f97a5d5f8b5722ea8a17d5d093211d7388b6&ipo=images" width="1200" height="500" class="d-block w-100">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Get Out of Depression</h5>
                    <p>exit the depressing mental state that is depression</p>
                </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
